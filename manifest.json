{
  "name": "Requirement Traceability Model for Agile Development",
  "version": "1.0",
  "description": "you can trace your activity",
  "content_scripts": [
    {
      "matches": ["https://trello.com/*"],
      "js": ["jquery-3.6.0.min.js", "tracechart.js"],
      "css": ["tracechart.css"]
    }
  ],
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": ["/images/trace_icon_png"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["storage"]
}
